{
  "name": "todo-electron-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "./app.js",
  "author": {
    "name": "Matt Gresham",
    "email": "matt.gresham@me.com",
    "url": "https://www.linkedin.com/in/matt-gresham/"
  },
  "build": {
    "appId": "com.matt-gresham.todo-electron-app",
    "productName": "TodoElectronApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
      "api/**/*",
      {
        "from": "./api/node_modules",
        "to": "api/node_modules",
        "filter": [
          "**/*"
        ]
      },
      "app.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -mw"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
    "recursive-install": "^1.4.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "electron": "^5.0.6",
    "electron-builder": "^21.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0"
  }
}
